<div id="overlay"></div>
<div id="loading">
	<div class="login">
		<form>
			<h3>Sign in to Google Mail</h3>
			<div class="username-wrapper">
				<label for="username">Email</label><input tabindex="1" id="username" type="text" placeholder="example@gmail.com" value="{{=it.username || ''}}">
			</div>
			<div class="password-wrapper">
				<label for="password">Password</label><input tabindex="2" id="password" type="password" placeholder="password" value="{{=it.password || ''}}">
			</div>
			<div class="username-wrapper">
				<label for="keybase_username">Keybase</label><input tabindex="3" id="keybase_username" type="text" placeholder="keybase username" value="{{=it.keybase_username || ''}}">
			</div>
			<div class="password-wrapper">
				<label for="keybase_password">Password</label><input tabindex="4" id="keybase_password" type="password" placeholder="keybase password" value="{{=it.password || ''}}">
			</div>
			<button type="submit" class="login-button">Sign in</button>
		</form>
	</div>
</div>
